   <pattern id="schematron-constraint-hwhPersons-residence-residence-no-mixed-content-60">
      <rule context="tei:residence">
         <report test="tei:*/tei:*">No further markup allowed within <name/>
         </report>
      </rule>
   </pattern>
   <pattern id="schematron-constraint-hwhPersons-subst-substContents1-61">
      <rule context="tei:subst">
         <assert test="child::tei:add and (child::tei:del or child::tei:surplus)">
