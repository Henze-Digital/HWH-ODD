               <data type="string">
                  <param name="pattern">A00[0-9A-F]{5}</param>
               </data>
            </attribute>
         </optional>
         <ref name="att.global.responsibility.attribute.cert"/>
         <empty/>
      </element>
   </define>
   <define name="placeName">
