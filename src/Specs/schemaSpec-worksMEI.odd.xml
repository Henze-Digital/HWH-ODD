    <elementSpec ident="title" module="MEI.shared" mode="change" ns="http://www.music-encoding.org/ns/mei">
        <desc versionDate="2021-12-08" xml:lang="de">Markiert einen Titel.</desc>
        <desc versionDate="2021-12-08" xml:lang="en">marks a title.</desc>
        <!--<classes mode="change">
            <memberOf key="att.authorized" mode="delete"/>
            <memberOf key="att.canonical" mode="delete"/>
            <memberOf key="att.bibl" mode="delete"/>
            <memberOf key="att.facsimile" mode="delete"/>
        </classes>-->
        <content>
            <rng:oneOrMore>
                <rng:ref name="titlePart"/>
            </rng:oneOrMore>
        </content>
        <constraintSpec scheme="schematron" ident="work-subtitles-map-xmllang" mode="delete">
            <constraint>
                <sch:rule context="mei:title[@type='sub'][parent::mei:titleStmt]">
                    <sch:assert test="string(@xml:lang) = preceding-sibling::mei:title/string(@xml:lang)">Make sure that this subtitle has a proper xml:lang attribute matching a preceding title's xml:lang</sch:assert>
                </sch:rule>
            </constraint>
        </constraintSpec>
        <attList>
            <attDef ident="type" mode="change" usage="req">
                <valList mode="change" type="closed">
                    <valItem ident="sub" mode="delete"/>
                    <valItem ident="div" mode="delete"/>
                    <valItem ident="reg" mode="add">
                        <gloss versionDate="2021-12-08" xml:lang="de">normiert</gloss>
                        <gloss versionDate="2021-12-08" xml:lang="en">regularized</gloss>
                        <desc versionDate="2021-12-08" xml:lang="de">Markiert einen Normierten Titel.</desc>
                        <desc versionDate="2021-12-08" xml:lang="en">marks a regularized title.</desc>
                    </valItem>
                </valList>
            </attDef>
        </attList>
    </elementSpec>
    <elementSpec ident="titlePart" module="MEI.shared" mode="add" ns="http://www.music-encoding.org/ns/mei">
        <desc versionDate="2021-12-08" xml:lang="de">Markiert einen Abschnitt eines Titels.</desc>
        <desc versionDate="2021-12-08" xml:lang="en">marks a part of a title.</desc>
        <!--<classes mode="change">
            <memberOf key="att.authorized" mode="delete"/>
            <memberOf key="att.canonical" mode="delete"/>
            <memberOf key="att.bibl" mode="delete"/>
            <memberOf key="att.facsimile" mode="delete"/>
        </classes>-->
        <content>
            <rng:text/>
        </content>
        <!--<constraintSpec scheme="schematron" ident="work-subtitles-" mode="add">
            <constraint>
                <sch:rule context="mei:title[@type='sub'][parent::mei:titleStmt]">
                    <sch:assert test="string(@xml:lang) = preceding-sibling::mei:title/string(@xml:lang)">Make sure that this subtitle has a proper xml:lang attribute matching a preceding title's xml:lang</sch:assert>
                </sch:rule>
            </constraint>
        </constraintSpec>-->
        <attList>
            <attDef ident="type" mode="add" usage="req">
                <valList type="closed">
                    <valItem ident="main" mode="add">
                        <gloss versionDate="2021-12-08" xml:lang="de">haupt</gloss>
                        <gloss versionDate="2021-12-08" xml:lang="en">main</gloss>
                        <desc versionDate="2021-12-08" xml:lang="de">Markiert einen (Haupt-)Titel.</desc>
                        <desc versionDate="2021-12-08" xml:lang="en">marks a (main) title.</desc>
                    </valItem>
                    <valItem ident="sub" mode="add">
                        <gloss versionDate="2021-12-08" xml:lang="de">untergeordnet</gloss>
                        <gloss versionDate="2021-12-08" xml:lang="en">subordinate</gloss>
                        <desc versionDate="2021-12-08" xml:lang="de">Markiert einen Untertitel.</desc>
                        <desc versionDate="2021-12-08" xml:lang="en">marks a subtitle.</desc>
                    </valItem>
                    <valItem ident="desc" mode="add">
                        <gloss versionDate="2021-12-08" xml:lang="de">beschreibend</gloss>
                        <gloss versionDate="2021-12-08" xml:lang="en">descriptive</gloss>
                        <desc versionDate="2021-12-08" xml:lang="de">Markiert einen beschreibenden Teil des Titels. (z.B. für Männerchor)</desc>
                        <desc versionDate="2021-12-08" xml:lang="en">marks a regularized title. (e.g., for string quartet)</desc>
                    </valItem>
                </valList>
            </attDef>
        </attList>
    </elementSpec>
    
